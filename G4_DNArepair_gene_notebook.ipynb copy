   "cell_type": "code",
   "execution_count": 99,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def reverse_comp(seq):\n",
    "    bases_dict = {'A':'T', 'T':'A', 'G':'C', 'C':'G'}\n",
    "    return ''.join(bases_dict.get(base,base) for base in seq[::-1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def remove_duplicate_G4(mydf):\n",
    "    \"\"\"compare G4 start with gene symbol, if they are the same, only keep one\"\"\"\n",
    "    to_drop=[]\n",
    "    for i in range(1,mydf.shape[0]):\n",
    "        if mydf.iloc[i,1]==mydf.iloc[i-1,1] and mydf.iloc[i,12] == mydf.iloc[i-1,12]:\n",
    "            to_drop.append(i)\n",
    "    for i in range(mydf.shape[0]):\n",
    "        if mydf.iloc[i,3]=='-':\n",
    "            mydf.iloc[i,4]=reverse_comp(mydf.iloc[i,4])\n",
    "    mydf=mydf.drop(mydf.index[to_drop])\n",
    "\n",
    "    return mydf\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
