  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def find_tss(mydf): \n",
    "    \"\"\"add a column of tss to mydf, return mydf, for downstream TSS\"\"\"\n",
    "    tss=np.zeros(mydf.shape[0])\n",
    "    for i in range(mydf.shape[0]):\n",
    "        if mydf.strand[i]=='-':\n",
    "            tss[i]=mydf.end[i]\n",
    "        else:\n",
    "            tss[i]=mydf.start[i]\n",
    "    mydf['tss']=tss\n",
    "    return mydf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def find_tss_upstream(mydf): \n",
    "    \"\"\"add a column of tss to mydf, return mydf\"\"\"\n",
    "    tss=np.zeros(mydf.shape[0])\n",
    "    for i in range(mydf.shape[0]):\n",
    "        if mydf.strand[i]=='-':\n",
    "            tss[i]=mydf.start[i]\n",
    "        else:\n",
    "            tss[i]=mydf.end[i]\n",
    "    mydf['tss']=tss\n",
    "    return mydf"
   ]
  },
